{
    "manifest_version": 3,
    "name": "YouTube Element Hider",
    "version": "1.0",
    "description": "Hide specific elements on YouTube.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  